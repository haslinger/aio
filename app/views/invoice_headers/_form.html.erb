<%= simple_form_for(@invoice_header) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_id %>
    <%= f.input :customer_id %>
    <%= f.input :reference_number %>
    <%= f.input :currency_id %>
    <%= f.input :created_by %>
    <%= f.input :consignee %>
    <%= f.input :dicount %>
    <%= f.input :discountvalue_header %>
    <%= f.input :discountvalue_position %>
    <%= f.input :taxvalue_full %>
    <%= f.input :taxvalue_half %>
    <%= f.input :valuevalue_netto_full %>
    <%= f.input :value_netto_half %>
    <%= f.input :value_netto %>
    <%= f.input :value_payed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
