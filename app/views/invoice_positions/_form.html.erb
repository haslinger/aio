<%= simple_form_for(@invoice_position) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :textposition %>
    <%= f.input :product_id %>
    <%= f.input :quantity %>
    <%= f.input :unit_id %>
    <%= f.input :discount %>
    <%= f.input :value %>
    <%= f.input :vatrate %>
    <%= f.input :price %>
    <%= f.input :base_price %>
    <%= f.input :description %>
    <%= f.input :created_by %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
